@{
    ViewBag.Title = "Eng4You | Contact";

}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Contact Form</title>
    <link rel="stylesheet" href="~/css/contact.css" />
    <script src="https://kit.fontawesome.com/64d58efce2.js"
            crossorigin="anonymous"></script>
    <link href="~/css/alert-send-email.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

</head>
<body>
    <div class="cont">
        <span class="big-circle"></span>
        <img src="~/images/bg.png" class="square" alt="" />
        <div class="form" dir="@Eng4You.Resources.Resource.dir">
            <div class="contact-info">
                <h3 class="title" style="font-weight:bold; text-align:start;position:relative">
                    @Eng4You.Resources.Resource.Contact_us_to_help_you___
                </h3>
                <p class="text" style="text-align:start;position:relative">
                    @Eng4You.Resources.Resource.Send_us_a_message_for_any_questions_you_have____You_will_be_reply_from_the_support_team_as_soon_as_you_receive_the_message📩❤️
                </p>

                <div class="info" style="text-align:start;position:relative">
                    <div class="information">
                        <p style="color:#000;"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 100 100"><path fill="#ffcc00" d="M50.001 0C33.65 0 20.25 13.36 20.25 29.666c0 6.318 2.018 12.19 5.433 17.016L46.37 82.445c2.897 3.785 4.823 3.066 7.232-.2l22.818-38.83c.46-.834.822-1.722 1.137-2.629a29.28 29.28 0 0 0 2.192-11.12C79.75 13.36 66.354 0 50.001 0m0 13.9c8.806 0 15.808 6.986 15.808 15.766c0 8.78-7.002 15.763-15.808 15.763c-8.805 0-15.81-6.982-15.81-15.763c0-8.78 7.005-15.765 15.81-15.765" /><path fill="#ffcc00" d="m68.913 48.908l-.048.126c.015-.038.027-.077.042-.115zM34.006 69.057C19.88 71.053 10 75.828 10 82.857C10 92.325 26.508 100 50 100s40-7.675 40-17.143c0-7.029-9.879-11.804-24.004-13.8l-1.957 3.332C74.685 73.866 82 76.97 82 80.572c0 5.05-14.327 9.143-32 9.143c-17.673 0-32-4.093-32-9.143c-.001-3.59 7.266-6.691 17.945-8.174c-.645-1.114-1.294-2.226-1.94-3.341" color="#ffcc00" /></svg>@Eng4You.Resources.Resource.Middle_East__Turkiye__USA</p>
                    </div>
                    <div class="information">
                        <p style="color:#000">
                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 36 36"><path fill="#ffcc00" d="M32.33 6a2 2 0 0 0-.41 0h-28a2 2 0 0 0-.53.08l14.45 14.39Z" class="clr-i-solid clr-i-solid-path-1" /><path fill="#ffcc00" d="m33.81 7.39l-14.56 14.5a2 2 0 0 1-2.82 0L2 7.5a2 2 0 0 0-.07.5v20a2 2 0 0 0 2 2h28a2 2 0 0 0 2-2V8a2 2 0 0 0-.12-.61M5.3 28H3.91v-1.43l7.27-7.21l1.41 1.41Zm26.61 0h-1.4l-7.29-7.23l1.41-1.41l7.27 7.21Z" class="clr-i-solid clr-i-solid-path-2" /><path fill="none" d="M0 0h36v36H0z" /></svg> @Eng4You.Resources.Resource.info_eng4you_gmail_com
                        </p>
                    </div>
                    <div class="information">
                        <p style="color:#000">
                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 12 12"><path fill="#ffcc00" d="m5.966 4.49l-.827.742a5 5 0 0 0 .455 1.073a4.7 4.7 0 0 0 .722.922l1.071-.33c.6-.185 1.255.005 1.654.48l.61.726a1.47 1.47 0 0 1-.137 2.042c-.995.908-2.527 1.215-3.674.314a10.4 10.4 0 0 1-2.516-2.87A10 10 0 0 1 2.03 4.013c-.22-1.422.821-2.56 2.119-2.948c.774-.232 1.6.166 1.884.908l.335.875c.22.576.062 1.225-.402 1.641" /></svg> @Eng4You.Resources.Resource.__972__0512532477

                        </p>
                    </div>
                </div>

                <div class="social-media">
                    <p style="font-weight: bold; color: #000; text-align: start; position: relative">@Eng4You.Resources.Resource.Contact_us_through_</p>
                    <div class="social-icons" style="text-align:start;position:relative">
                        <a href="#">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"><path fill="#fff" d="M20 18h-2V9.25L12 13L6 9.25V18H4V6h1.2l6.8 4.25L18.8 6H20m0-2H4c-1.11 0-2 .89-2 2v12a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V6a2 2 0 0 0-2-2" /></svg>
                        </a>

                        <a href="https://www.instagram.com/eng4you_world/">
                            <i class="fab fa-instagram"></i>
                        </a>
                    </div>
                </div>
            </div>

            <div class="contact-form">
                <span class="circle one"></span>
                <span class="circle two"></span>

                <form action="index.html" autocomplete="off" data-type="contactform">
                    <h3 class="title" style="position:relative;text-align:start">@Eng4You.Resources.Resource.Contact_Us___</h3><br />
                    <div class="input-container">
                        <input type="text" name="first_name" id="first_name" class="input" placeholder="@Eng4You.Resources.Resource.First_Name" required />
                    </div>
                    <div class="input-container">
                        <input type="text" name="last_name" id="last_name" class="input" placeholder="@Eng4You.Resources.Resource.Last_Name" required />
                    </div>
                    <div class="input-container">
                        <input type="email" name="email" id="email" class="input" placeholder="@Eng4You.Resources.Resource.Email" required />
                    </div>
                    <div class="input-container">
                        <input type="text" name="subject" id="subject" class="input" placeholder="@Eng4You.Resources.Resource.Subject" required />
                    </div>
                    <div class="input-container textarea">
                        <textarea name="message" class="input" id="message" required placeholder="@Eng4You.Resources.Resource.Message"></textarea>
                    </div>
                    <input type="submit" id="button" value="@Eng4You.Resources.Resource.Send" class="btn" />
                </form>
            </div>
        </div>
    </div>

    <script src="~/js/contact .js"></script>
    <script src="~/js/send-email.js"></script>
    <!--Emailjs-->
    <script type="text/javascript"
            src="https://cdn.jsdelivr.net/npm/emailjs-com@2/dist/email.min.js"></script>
    <script type="text/javascript">
        (function () {
            emailjs.init("Lj4rQmeH6ZVj-_Gj5");
        })();
    </script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Include SweetAlert library -->
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/js/js-contact/jquery.min.js"></script>
    <script src="~/js/js-contact/jquery.validate.min.js"></script>
    <script src="~/js/js-contact/main.js"></script>
    <script src="~/js/js-contact/popper.js"></script>
    <script src="~/js/translate-name-section.js"></script>
    <script src="~/js/index.js"></script>

</body>
</html>

